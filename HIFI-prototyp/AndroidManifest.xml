<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hifi_prototyp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:debuggable="true">
        <activity
            android:name=".PasswordReset"
            android:label="@string/title_activity_password_reset"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".UserProfile"
            android:label="@string/title_activity_user_profile"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".PokerTable"
            android:label="@string/title_activity_poker_table"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".LogIn2"
            android:label="@string/title_activity_log_in2"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Feedback"
            android:label="@string/title_activity_feedback"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".MainMenu"
            android:label="@string/application_title"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".PickCard"
            android:label="@string/application_title"
            android:screenOrientation="landscape" >
        </activity>
         <activity
            android:name=".PickCardWheel"
            android:label="@string/application_title"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".RegisterUser"
            android:label="@string/title_activity_register"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".Help"
            android:label="@string/title_activity_help"
            android:screenOrientation="landscape" >
        </activity> 
    </application>

</manifest>
